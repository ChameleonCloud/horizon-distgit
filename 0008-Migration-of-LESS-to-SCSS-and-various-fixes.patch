+++ b/openstack_dashboard/dashboards/theme/static/dashboard/scss/rcue/_navbar.scss
@@ -0,0 +1,468 @@
+.header.rcue {
+  border-top: 3px solid #cc0000;
+  position: relative;
+  .brand {
+    float: left;
+    padding: 7px 0;
+    margin: 0 0 0 20px;
+    text-shadow: none;
+    img {
+      display: block;
+      font-size: 11px;
+    }
+  }
+  .caret {
+    border: 0;
+    font-family: $icon-font-name-fa;
+    font-weight: normal;
+    height: $font-size-base - 3;
+    margin-top: 0;
+    opacity: 1;
+    position: relative;
+    vertical-align: baseline;
+    text-indent: 0;
+    width: $font-size-base;
+    filter: none;
+    &:before {
+      bottom: 0;
+      content: "\f107";
+      left: 0;
+      line-height: $font-size-base;
+      position: absolute;
+      text-align: center;
+      top: -1px;
+      right: 0;
+    }
+  }
+  .navbar {
+    border: 0;
+    border-radius: 0;
+    margin-bottom: 0;
+    min-height: 0;
+    &.primary {
+      font-size: 13px;
+      &.persistent-secondary {
+        .nav {
+          position: static;
+          .dropup .dropdown-menu {
+            bottom: 0;
+            top: auto;
+          }
